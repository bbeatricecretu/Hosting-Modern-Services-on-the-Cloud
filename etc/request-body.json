{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": [
        "s3:PutObject",
        "s3:PutObjectLegalHold",
        "s3:PutObjectRetention",
        "s3:PutObjectTagging",
        "s3:PutObjectVersionTagging",
        "s3:Abort*"
      ],
      "Resource": "arn:aws:s3:::<s3-mhpbucketbeatrice>/*",
      "Effect": "Allow"
    },
    {
      "Action": [
        "s3:GetObject*",
        "s3:GetBucket*",
        "s3:List*"
      ],
      "Resource": [
        "arn:aws:s3:::<s3-mhpbucketbeatrice>",
        "arn:aws:s3:::<s3-mhpbucketbeatrice>/*"
      ],
      "Effect": "Allow"
    },
    {
      "Action": [
        "dynamodb:BatchGetItem",
        "dynamodb:GetRecords",
        "dynamodb:GetShardIterator",
        "dynamodb:Query",
        "dynamodb:GetItem",
        "dynamodb:Scan",
        "dynamodb:ConditionCheckItem",
        "dynamodb:BatchWriteItem",
        "dynamodb:PutItem",
        "dynamodb:UpdateItem",
        "dynamodb:DeleteItem",
        "dynamodb:DescribeTable"
      ],
      "Resource": [
        "arn:aws:dynamodb:eu-west-1:<982590065959>:table/<MHp_dynamo_db>",
        "arn:aws:dynamodb:eu-west-1:<982590065959>:table/<MHp_dynamo_db>/index/*"
      ],
      "Effect": "Allow"
    },
    {
      "Action": [
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": "arn:aws:s3:::<s3-mhpbucketbeatrice>/*",
      "Effect": "Allow"
    }
  ]
}